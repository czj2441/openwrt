config init
        option stdout '1'
        option stderr '1'
        option user 'root'
        option group 'root'
        option respawn '1'

config conf 'common'
        option server_addr 'srv.ebeam.vip'
        option server_port '47654'
        option token 'lemonade@mynas'
        option user 'e8820s'
        option protocol 'tcp'
        option tls_enable 'true'
        list _ 'authentication_method=token'
        list _ 'authenticate_heartbeats=true'
        list _ 'authenticate_new_work_conns=true'

config conf 'ssh'
        option type 'tcp'
        option local_ip '127.0.0.1'
        option local_port '22'
        option remote_port '39320'
        option name 'ssh'
        option use_encryption 'false'
        option use_compression 'false'

config conf
        option name 'luci'
        option type 'tcp'
        option use_encryption 'true'
        option use_compression 'false'
        option local_ip '127.0.0.1'
        option local_port '80'
        option remote_port '39380'
